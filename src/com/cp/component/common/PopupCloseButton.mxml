<?xml version="1.0" encoding="utf-8"?>
<s:Image xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 source.down="@Embed('/asset/resource.swf#closeDown')"
		 source.normal="@Embed('/asset/resource.swf#closeUp')"
		 touchBegin="currentState = 'down'"
		 touchEnd="currentState = 'normal'"
		 >

	<s:states>
		<s:State name="normal" />
		<s:State name="down" />
	</s:states>

</s:Image>
